{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.5.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": ""
    },
    {
      "type": "panel",
      "id": "state-timeline",
      "name": "State timeline",
      "version": ""
    },
    {
      "type": "panel",
      "id": "barchart",
      "name": "Bar chart",
      "version": ""
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "iteration": 1,
  "links": [],
  "panels": [
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "percentage",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "100 * avg(pingsanto_agent_ready{site=~\"$site\",agent_id=~\"$agent\"})",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Agents Ready (%)",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "sum"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "count_values(\"agent\", (1 - pingsanto_agent_ready{site=~\"$site\",agent_id=~\"$agent\"}))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Agents Not Ready",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "increase(pingsanto_agent_ready_alerts_total{site=~\"$site\",agent_id=~\"$agent\"}[24h])",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Readiness Alerts (24h)",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "id": 4,
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Not Ready"
          }
        ]
      },
      "pluginVersion": "9.5.0",
      "targets": [
        {
          "expr": "pingsanto_agent_ready{site=~\"$site\",agent_id=~\"$agent\"}",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        },
        {
          "expr": "pingsanto_agent_ready_info{site=~\"$site\",agent_id=~\"$agent\"}",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "B"
        },
        {
          "expr": "pingsanto_agent_ready_categories_info{site=~\"$site\",agent_id=~\"$agent\",severity=~\"$severity\"}",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "C"
        }
      ],
      "title": "Agents with Readiness Issues",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "indexByName": {
              "Value #A": 3,
              "Value #B": 4,
              "Value #C": 5,
              "agent_id": 0,
              "env": 2,
              "isp": 1,
              "site": 6
            },
            "renameByName": {
              "Value #A": "Not Ready",
              "Value #B": "Ready Reason",
              "Value #C": "Categories"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "id": 5,
      "options": {
        "displayMode": "lines",
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "pingsanto_agent_ready{site=~\"$site\",agent_id=~\"$agent\"}",
          "legendFormat": "{{agent_id}}",
          "refId": "A"
        }
      ],
      "title": "Readiness State Timeline",
      "type": "state-timeline"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "severity"
            },
            "properties": [
              {
                "id": "custom.axisPlacement",
                "value": "hidden"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "id": 6,
      "options": {
        "orientation": "horizontal",
        "showValue": "always",
        "stacking": "normal"
      },
      "targets": [
        {
          "expr": "sum by (category, severity) (pingsanto_agent_ready_categories_info{site=~\"$site\",severity=~\"$severity\",agent_id=~\"$agent\"})",
          "legendFormat": "{{category}} ({{severity}})",
          "refId": "A"
        }
      ],
      "title": "Active Readiness Categories",
      "type": "barchart"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "id": 7,
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Transitions"
          }
        ]
      },
      "targets": [
        {
          "expr": "increase(pingsanto_agent_ready_category_transitions_total{site=~\"$site\",severity=~\"$severity\",agent_id=~\"$agent\"}[24h])",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Category Transitions (24h)",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "indexByName": {
              "Value": 3,
              "agent_id": 0,
              "category": 1,
              "severity": 2,
              "site": 4
            },
            "renameByName": {
              "Value": "Transitions"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "id": 8,
      "targets": [
        {
          "expr": "rate(pingsanto_agent_ready_alerts_total{site=~\"$site\",agent_id=~\"$agent\"}[5m])",
          "legendFormat": "Alert rate",
          "refId": "A"
        }
      ],
      "title": "Readiness Alerts Rate",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "id": 9,
      "targets": [
        {
          "expr": "rate(pingsanto_agent_ready_transitions_total{state=\"ready\",site=~\"$site\",agent_id=~\"$agent\"}[5m])",
          "legendFormat": "Ready transitions",
          "refId": "A"
        },
        {
          "expr": "rate(pingsanto_agent_ready_transitions_total{state=\"not_ready\",site=~\"$site\",agent_id=~\"$agent\"}[5m])",
          "legendFormat": "Not Ready transitions",
          "refId": "B"
        }
      ],
      "title": "Readiness Transition Rates",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": [
    "pingsanto",
    "readiness"
  ],
